- if user.present?
  = link_to_unless( user.nickname.blank?, image_tag( user.image, size: size, class: "header-right-menu__icon-image"), "#{Settings[user.provider + '_page']}#{user.nickname}", target: "_blank", title: user.nickname, rel: "tooltip", "data-original-title" => user.nickname, class: "header-right-menu__icon-link")
